#Method 1

newlist=list(range(1,40))

print(newlist)

def reverse(newlist):

	newlist.reverse()
	return newlist
	
print(reverse(newlist))

# Method 2

# Reversing a list using slicing technique 

def reverse(newlist): 
    new_list = newlist[::-1] 
    return new_list 
      
newlist = [10, 11, 12, 13, 14, 15]

print(Reverse(newlist)) 


# Method 3 

mylist=list(range(1,201))

print(mylist[::-1])
